<Window x:Class="LearningPractice_IE_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LearningPractice_IE_"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1000"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="8*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="IEButton" Grid.Row="0" Grid.Column="0" Content="Информация о ИП" Height="30" Width="140" Click="IEButton_Click"/>
        <Button x:Name="IncomeExpensesButton" Grid.Row="0" Grid.Column="1" Content="Доходы/Расходы" Height="30" Width="140" Click="IncomeExpensesButton_Click"/>
        <Frame x:Name="MainFrame" Grid.Row="1" Grid.ColumnSpan="3" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
